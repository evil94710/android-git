<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.colormatch">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".app_home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Logo"
            android:label="|  LOGO  |"
            android:largeHeap="true"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PPT"
            android:label="|  PPT  |"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Collect"
            android:label="@string/title_activity_collect"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".add_ppt"
            android:label="@string/title_activity_add_ppt"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".add_logo"
            android:label="@string/title_activity_add_logo"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>